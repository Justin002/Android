<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_bg"
    android:orientation="vertical" >

    <include layout="@layout/title_layout_right_txt" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="150dip"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="48dip"
                    android:layout_height="48dip"
                    android:layout_margin="@dimen/MSpace"
                    android:src="@drawable/icon_congratulations" />

                <TextView
                    android:id="@+id/tv_repay_result"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/congratulations_reimbursement_success"
                    android:textColor="@color/dark"
                    android:textSize="@dimen/font_btn_lll" />

                <TextView
                    android:id="@+id/tv_borrowCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="D15110119a"
                    android:textColor="@color/e_text_bg_huise_black"
                    android:textSize="@dimen/font_btn_0" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_repay_details"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/HSpace"
                android:background="@drawable/icon_congratulations_bg"
                android:orientation="vertical"
                android:paddingBottom="40dip"
                android:paddingLeft="@dimen/nomalSpace"
                android:paddingRight="@dimen/nomalSpace"
                android:paddingTop="@dimen/XXHSpace" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="45dip"
                    android:background="@color/white"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/HSpace2"
                    android:paddingRight="@dimen/HSpace2" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/reimbursement_date"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_h" />

                    <View
                        android:layout_width="0dip"
                        android:layout_height="1dip"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/tv_repaidTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="2015-09-15"
                        android:textColor="@color/dark"
                        android:textSize="@dimen/font_text_h" />
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_grey" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/HSpace2"
                    android:paddingRight="@dimen/HSpace2" >

                    
                    <LinearLayout 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center_vertical"
                        >
                        <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        
                        android:text="@string/money_amount_benjin"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_h" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:text="@string/money_amount_benjin_left_all"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_ll" />

                    
                    <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    </LinearLayout>

                    <View
                        android:layout_width="0dip"
                        android:layout_height="1dip"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/tv_capital"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4800.00元"
                        android:textColor="@color/text_color"
                        android:textSize="@dimen/font_text_h" />
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_grey" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/HSpace2"
                    android:paddingRight="@dimen/HSpace2" >

                   
                    <LinearLayout 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                    <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        
                        android:text="@string/interest"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_h" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:id="@+id/tv_prepaymentInterest"
                        android:text="@string/interest_detail"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_ll" />

                   
                    <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    </LinearLayout>

                    <View
                        android:layout_width="0dip"
                        android:layout_height="1dip"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/tv_interest"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4800.00元"
                        android:textColor="@color/text_color"
                        android:textSize="@dimen/font_text_h" />
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_grey" />
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/HSpace2"
                    android:paddingRight="@dimen/HSpace2" >

                   
                    <LinearLayout 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                    <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                       
                        android:text="@string/manage_money"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_h" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/manage_money_detail"
                        android:id="@+id/tv_prepaymentManage"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_ll" />

                   
                    <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    </LinearLayout>

                    <View
                        android:layout_width="0dip"
                        android:layout_height="1dip"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/tv_managementCost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4800.00元"
                        android:textColor="@color/text_color"
                        android:textSize="@dimen/font_text_h" />
                </LinearLayout>
                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_grey" />
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:layout_gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/HSpace2"
                    android:paddingRight="@dimen/HSpace2" >

                    
                    <LinearLayout 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="vertical">
                    <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        
                        android:text="@string/my_huankuan_xiangqing_faxi"
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_h" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/my_huankuan_xiangqing_faxi_profit"
                        
                        android:textColor="@color/e_text_huise_buttom"
                        android:textSize="@dimen/font_text_ll" />

                    <ListView
                        android:id="@+id/lt_amount_faxi"
                        android:paddingLeft="@dimen/HSpace"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        
                         />
                    <LinearLayout 
                        android:layout_width="fill_parent"
                        android:layout_height="10dp"/>
                    </LinearLayout>

                    <View
                        android:layout_width="0dip"
                        android:layout_height="1dip"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/tv_latefee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4800.00元"
                        android:layout_gravity="center_vertical"
                        android:textColor="@color/text_color"
                        android:textSize="@dimen/font_text_h" />
                </LinearLayout>
                
                
            </LinearLayout>

            <Button
                android:id="@+id/bt_look_reimbursement_finish"
                android:layout_width="fill_parent"
                android:layout_height="45dip"
                android:layout_marginBottom="@dimen/XHSpace"
                android:layout_marginLeft="@dimen/XHSpace"
                android:layout_marginRight="@dimen/XHSpace"
                android:layout_marginTop="33dip"
                android:background="@drawable/shape_button_bg_selector"
                android:text="@string/look_reimbursement_finish"
                android:textColor="@color/white"
                android:textSize="@dimen/font_btn_mm" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>