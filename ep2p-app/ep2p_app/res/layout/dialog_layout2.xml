<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/ll_layout_dialog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/e_img_bg_dig_a03"
        android:layout_centerVertical="true"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/app_dialog_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_marginTop="95dp"
            android:text="@string/txt_tv_zc_rz2"
            android:textColor="@color/e_text_dialog_yellow"
            android:textSize="@dimen/font_btn_ll" />

        <!-- 
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="95dp"
            android:text="@string/txt_tv_zc_gxrz2"
            android:textColor="@color/e_text_dialog_yellow"
            android:textSize="@dimen/font_btn_ll" /> -->

        <TextView
            android:id="@+id/app_dialog_descri"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="95dp"
            android:textColor="@color/e_text_dialog_yellow"
            android:textSize="@dimen/font_btn_ll" />
    </LinearLayout>

</RelativeLayout>